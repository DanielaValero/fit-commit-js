{
  "name": "fit-commit-js",
  "version": "0.0.1",
  "description": "An npm package to validate commits according to community guidelines",
  "author": "Daniela Valero",
  "license": "Apache Version 2.0",
  "main": "bin/fit-commit-js.js",
  "scripts": {
    "style": "jscs -v lib/*.js",
    "test": "NODE_ENV=test mocha --harmony --recursive --ui tdd --reporter dot",
    "watch": "NODE_ENV=test mocha debug --recursive  --watch --reporter dot --harmony",
    "debugTest": "NODE_ENV=test node-debug _mocha --recursive  --ui tdd --reporter dot"
  },
  "engines": {
    "node": ">=4.1.0"
  },
  "fitCommitJs": {
    "file": "config/config.defualt.yml"
  },
  "keywords": [
    "git",
    "commit",
    "fit-commit"
  ],
  "devDependencies": {
    "debug": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "is-my-json-valid": "^2.12.3",
    "js-yaml": "^3.4.6",
    "mocha": "^2.3.4",
    "npmlog": "^2.0.0",
    "ramda": "^0.18.0",
    "sinon": "^1.17.2"
  }
}
